{
  "extends": ["../../typedoc.base.json"],
  "entryPoints": [
    "lib/index.ts",
    "lib/RpcApi.ts",
    "lib/RpcSerialPort.ts",
    "lib/ScreenFrame.ts",
    "lib/Types.ts",
    "lib/StreamProtobufReader.ts",
    "lib/Utils.ts",
    "lib/MatchProtobufVersion.ts"
  ],
  "intentionallyNotExported": ["lib/MatchProtobufVersion.ts:PB_Main"],
  "plugin": ["@flipper-rpc-client/typedoc-plugin"],
  "tsconfig": "tsconfig.typedoc.json",
  "compilerOptions": {
    "paths": {
      "@flipper-rpc-client/commands": ["../commands/lib/index.ts"],
      "@flipper-rpc-client/commands/*": [
        "../commands/lib/*.ts",
        "../commands/lib/*/index.ts"
      ],
      "@flipper-rpc-client/core": ["./lib/index.ts"],
      "@flipper-rpc-client/core/*": ["./lib/*.ts", "../lib/*/index.ts"]
    }
  }
}
