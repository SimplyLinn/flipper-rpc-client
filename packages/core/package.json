{
  "name": "@flipper-rpc-client/core",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./RpcApi": {
      "require": "./dist/cjs/RpcApi.js",
      "import": "./dist/esm/RpcApi.js",
      "types": "./dist/types/RpcApi.d.ts"
    },
    "./Commands": {
      "require": "./dist/cjs/Commands.js",
      "import": "./dist/esm/Commands.js",
      "types": "./dist/types/Commands.d.ts"
    },
    "./Utils": {
      "require": "./dist/cjs/Utils.js",
      "import": "./dist/esm/Utils.js",
      "types": "./dist/types/Utils.d.ts"
    },
    "./MatchProtobufVersion": {
      "require": "./dist/cjs/MatchProtobufVersion.js",
      "import": "./dist/esm/MatchProtobufVersion.js",
      "types": "./dist/types/MatchProtobufVersion.d.ts"
    },
    "./RpcSerialPorti": {
      "require": "./dist/cjs/RpcSerialPort.js",
      "import": "./dist/esm/RpcSerialPort.js",
      "types": "./dist/types/RpcSerialPort.d.ts"
    },
    "./ScreenFrame": {
      "require": "./dist/cjs/ScreenFrame.js",
      "import": "./dist/esm/ScreenFrame.js",
      "types": "./dist/types/ScreenFrame.d.ts"
    },
    "./Types": {
      "require": "./dist/cjs/Types.js",
      "import": "./dist/esm/Types.js",
      "types": "./dist/types/Types.d.ts"
    },
    "./Errors": {
      "require": "./dist/cjs/Errors.js",
      "import": "./dist/esm/Errors.js",
      "types": "./dist/types/Errors.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build-all": "pnpm run \"/^build:.*/\"",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "lint-eslint": "eslint lib/**/*",
    "lint-typescript": "tsc",
    "lint-language": "alex lib/**/*",
    "prettier-check": "prettier --check lib/**/*",
    "test": "jest",
    "docs": "typedoc --out docs"
  },
  "dependencies": {
    "@flipper-rpc-client/commands": "workspace:*",
    "@flipper-rpc-client/versioned-protobuf": "workspace:*",
    "protobufjs": "7.2.5"
  },
  "devDependencies": {
    "@flipper-rpc-client/typedoc-plugin": "workspace:*",
    "@types/jest": "29.5.10",
    "jest": "29.7.0",
    "long": "5.2.3",
    "ts-jest": "29.1.1"
  }
}
